{
  "version": 2,
  "builds": [
    {
      "src": "backend/index.js",
      "use": "@vercel/node",
      "port": 3001
    },
    {
      "src": "frontend/index.js",
      "use": "@vercel/react",
      "port": 5173
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}